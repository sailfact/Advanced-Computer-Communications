tcpserv01.out: error.o wrapunix.o wrapsock.o wraplib.o wrapstdio.o tcpserv01.o readline.o writen.o str_file.o
	cc -o tcpserv01.out str_file.o error.o wrapunix.o wrapsock.o wraplib.o wrapstdio.o tcpserv01.o readline.o writen.o

error.o: error.c acc.h
	cc -c error.c

wrapunix.o: wrapunix.c acc.h
	cc -c wrapunix.c

wrapsock.o: wrapsock.c acc.h
	cc -c wrapsock.c

wraplib.o: wraplib.c acc.h
	cc -c wraplib.c

wrapstdio.o: wrapstdio.c acc.h
	cc -c wrapstdio.c

tcpserv01.o: tcpserv01.c acc.h
	cc -c tcpserv01.c

str_file.o: str_file.c acc.h
	cc -c str_file.c

readline.o: readline.c acc.h
	cc -c readline.c

writen.o: writen.c acc.h
	cc -c writen.c
